@model CoreLayout.Models.Masters.BranchModel
@{
    ViewData["Title"] = "Details";
}

<!-- Page Header -->
<div class="page-header">
    <div>
        <h2 class="main-content-title tx-24 mg-b-5">Branch Master Details</h2>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="#">Branch</a></li>
            <li class="breadcrumb-item active" aria-current="page">Details</li>
        </ol>
    </div>
</div>
<!-- End Page Header -->
<!-- Conetnt Start-->
<div class="row">
    <div class="col-lg-12 col-md-12">
        <div class="card custom-card">
            <div class="card-body">
                @*<div>
                        <h6 class="card-title mb-1">State Details</h6>
                        <hr />
                    </div>*@

                <form asp-action="Details" method="post">
                    <div class="row">
                        <input type="hidden" asp-for="BranchID" />
                        <div asp-validation-summary="ModelOnly" class="text-danger">

                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="BranchCode"> </label><span class="tx-danger"> *</span>
                                <input asp-for="BranchCode" class="form-control" readonly="readonly" />
                                <span class="parsley-errors-list filled" asp-validation-for="BranchCode"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="BranchName"> </label><span class="tx-danger"> *</span>
                                <input asp-for="BranchName" class="form-control"  readonly="readonly" />
                                <span class="parsley-errors-list filled" asp-validation-for="BranchName"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="HindiName"> </label><span class="tx-danger"> *</span>
                                <input asp-for="HindiName" class="form-control"  readonly="readonly" />
                                <span class="parsley-errors-list filled" asp-validation-for="HindiName"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="CerificateName"> </label><span class="tx-danger"> *</span>
                                <input asp-for="CerificateName" class="form-control"  readonly="readonly" />
                                <span class="parsley-errors-list filled" asp-validation-for="CerificateName"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="DisplayName"> </label><span class="tx-danger"> *</span>
                                <input asp-for="DisplayName" class="form-control"  readonly="readonly" />
                                <span class="parsley-errors-list filled" asp-validation-for="DisplayName"></span>
                            </div>
                        </div>
                        <div class="form-group">
                            <a asp-action="Edit" asp-route-id="@Model.EncryptedId" class="btn btn-primary">Edit</a>
                            <a asp-action="Index" class="btn btn-success">Back to List</a>
                        </div>
                    </div>

                </form>
            </div>
        </div>
    </div>
</div>
<!-- Conetnt End-->
