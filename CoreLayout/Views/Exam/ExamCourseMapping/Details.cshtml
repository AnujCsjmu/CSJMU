@model CoreLayout.Models.Exam.ExamCourseMappingModel
@{
    ViewData["Title"] = "Details";
}
<!-- Page Header -->
<div class="page-header">
    <div>
        <h2 class="main-content-title tx-24 mg-b-5">Exam Course Mapping Details</h2>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="#">ExamCourseMapping</a></li>
            <li class="breadcrumb-item active" aria-current="page">Details</li>
        </ol>
    </div>
</div>
<!-- End Page Header -->
<!-- Conetnt Start-->
<div class="row">
    <div class="col-lg-12 col-md-12">
        <div class="card custom-card">
            <div class="card-body">
                @*<div>
                        <h6 class="card-title mb-1">State Details</h6>
                        <hr />
                    </div>*@
                <form asp-action="Details" method="post">
                    <div class="row">
                        <input type="hidden" asp-for="ECId" />
                        <div asp-validation-summary="ModelOnly" class="parsley-errors-list filled">

                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="ExamId"> </label><span class="tx-danger"> *</span>
                                <input asp-for="ExamName" class="form-control" readonly="readonly" />
                                <span class="parsley-errors-list filled" asp-validation-for="ExamId"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="CourseID"> </label><span class="tx-danger"> *</span>
                                <input asp-for="CourseName" class="form-control" readonly="readonly" />
                                <span class="parsley-errors-list filled" asp-validation-for="CourseID"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="SemYearId"> </label><span class="tx-danger"> *</span>
                                <input asp-for="SemYearId" class="form-control" readonly="readonly" />
                                <span class="parsley-errors-list filled" asp-validation-for="SemYearId"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="PaperSetterPermission"> </label><span class="tx-danger"> *</span>
                                <input asp-for="PaperSetterPermission" class="form-control" readonly="readonly" />
                                <span asp-validation-for="PaperSetterPermission" class="parsley-errors-list filled"></span>
                            </div>
                        </div>
                        <div class="form-group">
                            <a asp-action="Edit" asp-route-id="@Model.EncryptedId" class="btn btn-primary">Edit</a>
                            <a asp-action="Index" class="btn btn-success">Back to List</a>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<!-- Conetnt End-->
