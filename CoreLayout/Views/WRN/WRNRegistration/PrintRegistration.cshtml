    @*view Qualification*@
    <h4>Qualification Details</h4>
    <div class="row">
        <div class="col-lg-12">
            <div class="card custom-card overflow-hidden">
                <div class="card-body">
                    <div class="table-responsive-sm">
                        <table id="exportexample" class="table table-sm table-bordered key-buttons text-wrap w-100">
                            <thead>
                                <tr>
                                    <th>S.No</th>
                                    <th>
                                        @Html.DisplayNameFor(c => c.RegistrationNo)
                                    </th>
                                    <th>
                                        Qualification
                                    </th>
                                    <th>
                                        Board /<br />University
                                    </th>
                                    <th>
                                        Result<br />Subjects
                                    </th>
                                    <th>
                                        Month<br />Year
                                    </th>
                                    <th>
                                        Percentage<br />Division
                                    </th>
                                    <th>
                                        Attachment
                                    </th>
                                    <th>
                                        Entry Date
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                @{
                                    int sno = 0;
                                }
                                @if (Enumerable.Count(ViewBag.qualificationdata) == 0)
                                {
                                    <tr>
                                        <td colspan="9" style="text-align:center">
                                            <span style="font-weight:bold;color:red">No Record Found</span>
                                        </td>
                                    </tr>
                                }
                                else
                                {

                                    @foreach (var item in ViewBag.qualificationdata)
                                    {
                                        <tr>
                                            <td>
                                                @{ sno++; }
                                                @sno
                                            </td>
                                            <td>@item.RegistrationNo</td>
                                            <td>@item.Qualification</td>
                                            <td>@item.University</td>
                                            <td>@item.ResultStatus<br />@item.Subjects</td>
                                            <td>@item.PassingMonth<br />@item.PassingYear</td>
                                            <td>@item.PercentagOfMarksObtained<br />@item.DivisionClassGrade</td>
                                            @if (item.MarksheetAttachmentPath != null)
                                            {
                                                <td><img src="~/WRNMarksheet/@item.MarksheetAttachmentPath" alt="Image" height="50" width="50" /></td>
                                            }
                                            else
                                            {
                                                <td></td>
                                            }
                                            <td>@item.CreatedDate</td>
                                        </tr>
                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
