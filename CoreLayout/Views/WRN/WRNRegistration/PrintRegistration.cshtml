<form method="post" asp-action="PrintRegistration">
    <div class="row">
        <div class="col-lg-12">
            <div class="form-group">
                <input type="submit" class="btn btn-primary" value="Print" onclick="printDiv('PrintDiv')" />
                <a asp-action="Dashboard" asp-controller="WRNDashboard" class="btn btn-success">Back to List</a>
            </div>
        </div>
    </div>
</form>
@section Scripts{
    <script language="javascript">
        function printDiv(divName) {
            var printContents = document.getElementById(divName).innerHTML;
            var originalContents = document.body.innerHTML;

            document.body.innerHTML = printContents;

            window.print();

            document.body.innerHTML = originalContents;
        }
    </script>
}
