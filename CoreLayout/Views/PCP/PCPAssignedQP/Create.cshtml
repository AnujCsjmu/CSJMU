@model CoreLayout.Models.PCP.PCPAssignedQPModel
@{
    ViewData["Title"] = "Create";
}

<!-- Page Header -->
<div class="page-header">
    <div>
        <h2 class="main-content-title tx-24 mg-b-5">Assigned QP to Paper Setter</h2>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="#">AssignedQP</a></li>
            <li class="breadcrumb-item active" aria-current="page">Create</li>
        </ol>
    </div>

</div>
<!-- End Page Header -->
<!-- Conetnt Start-->
<div class="row">
    <div class="col-lg-12 col-md-12">
        <div class="card custom-card">
            <div class="card-body">
                @*<div>
                        <h6 class="card-title mb-1">Create State</h6>
                        <hr>
                    </div>*@
                <form asp-action="Create" method="post">
                    <div class="row">
                        <div asp-validation-summary="ModelOnly" class="parsley-errors-list filled">

                        </div>

                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="QPId"> </label><span class="tx-danger"> *</span>
                                <select class="form-control select"  asp-for="QPId" asp-items="@(new SelectList(Model.QPList,"QPId","QPName"))">
                                    <option selected="selected" disabled="disabled" value="">--Select-- </option>
                                </select>
                                <span asp-validation-for="QPId" class="parsley-errors-list filled"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="PCPRegID"> </label><span class="tx-danger"> *</span>
                                <select class="form-control select" multiple asp-for="UserList" asp-items="@(new SelectList(ViewBag.UserList,"PCPRegID","UserName"))">
                                    @*<option selected="selected" disabled="disabled" value="">--Select-- </option>*@
                                </select>
                                <span asp-validation-for="PCPRegID" class="parsley-errors-list filled"></span>
                            </div>
                        </div>

                    </div>
                    <div class="form-group">
                        <input type="submit" class="btn btn-primary" value="Save" />
                        <a asp-action="Index" class="btn btn-success">Back to List</a>

                    </div>

                </form>
            </div>
        </div>
    </div>
</div>
<!-- Conetnt End-->
@section Scripts{
   
}

