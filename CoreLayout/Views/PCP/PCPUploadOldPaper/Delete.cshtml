@model CoreLayout.Models.PCP.PCPUploadOldPaperModel
@{
    ViewData["Title"] = "Delete";
}
<div class="main-content side-content pt-0">
    <div class="side-app">

        <div class="main-container container-fluid">


            <!-- Page Header -->
            <div class="page-header">
                <div>
                    <h2 class="main-content-title tx-24 mg-b-5">Delete Old Paper</h2>
                    <hr />
                </div>

            </div>
            <!-- End Page Header -->
            <!-- Conetnt Start-->
            <div class="row">
                <div class="col-lg-12 col-md-12">
                    <div class="card custom-card">
                        <div class="card-body">
                            <div>
                                <h6 class="card-title mb-1">Delete Assigned QP</h6>
                                <hr />
                            </div>



                            <form asp-action="Details" method="post">
                                <div class="row">
                                    <input type="hidden" asp-for="OldPaperId" />
                                    <div asp-validation-summary="ModelOnly" class="text-danger">

                                    </div>
                                    <div class="form-group">
                                        <a asp-action="Edit" asp-route-id="@Model.EncryptedId" class="btn btn-primary">Edit</a>
                                        <a asp-action="Index" class="btn btn-success">Back to List</a>
                                    </div>
                                </div>

                            </form>

                        </div>
                    </div>
                </div>
            </div>
            <!-- Conetnt End-->

        </div>
    </div>
</div>