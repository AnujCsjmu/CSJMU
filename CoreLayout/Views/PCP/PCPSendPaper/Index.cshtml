@using CoreLayout.Models.PCP
@model IEnumerable<PCPSendPaperModel>
@{
    ViewData["title"] = "Index";
}
<div class="page-header">
    <div>
        <h2 class="main-content-title tx-24 mg-b-5">Send Paper List</h2>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="#">SendPaper</a></li>
            <li class="breadcrumb-item active" aria-current="page">List</li>
        </ol>
    </div>
    <div class="btn btn-list">
        <a class="btn ripple btn-success btn-sm" asp-action="Create" asp-route-id=@ViewBag.MaxSendPaperId><i class="fe fe-plus"></i> Send Paper</a>
    </div>
</div>

<!-- Row -->
<div class="row">
    <div class="col-lg-12">
        <div class="card custom-card overflow-hidden">
            <div class="card-body">
                <div class="table-responsive-sm">
                    <table id="exportexample" class="table table-sm table-bordered key-buttons text-wrap w-100">
                        <thead>
                            <tr>
                                <th>S.No</th>
                                <th>
                                    @Html.DisplayNameFor(c => c.UserId)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(c => c.QPCode)
                                </th>

                                <th>
                                    @Html.DisplayNameFor(c => c.CourseId)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(c => c.BranchName)
                                </th>

                                <th>
                                    @Html.DisplayNameFor(c => c.Session)<br />
                                    @Html.DisplayNameFor(c => c.SemYearId)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(c => c.PaperOpenTime)
                                  
                                  
                                </th>
                                <th>
                                    @Html.DisplayNameFor(c => c.StaticIPAddress)
                                </th>

                                @*<th>
                                        @Html.DisplayNameFor(c => c.PaperPassword)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(c => c.PaperPath)
                                    </th>*@

                                <th>
                                    @Html.DisplayNameFor(c => c.CreatedDate)
                                </th>
                                <th>
                                    Action
                                </th>
                            </tr>
                        </thead>

                        <tbody>
                            @{
                                int sno = 0;
                            }
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @{ sno++; }
                                        @sno
                                    </td>
                                    <td>@item.AgencyName</td>
                                    <td>@item.QPCode</td>

                                    <td>@item.CourseName</td>
                                    <td>@item.BranchName</td>
                                    <td>@item.Session <br /> @item.SemYearId</td>
                                    <td>@item.PaperOpenTime</td>
                                    <td>@item.StaticIPAddress</td>
                                    @*<td>
                                            <b>Encrypt</b> <br />@item.PaperPassword<br />
                                            @if (ViewBag.EncryptPwdList != null)
                                            {
                                                foreach (var item1 in ViewBag.EncryptPwdList)
                                                {
                                                    <b>Decrypt</b> <br />@item.DecryptPassword
                                                    break;
                                                }
                                            }
                                        </td>
                                        <td>
                                            @if (item.PaperPath != null)
                                            {
                                                <img src="~/Images/pdfimage.png" height=30 width=30 /><br />
                                                <a asp-action="Download" asp-route-id="@item.EncryptedId" target="_blank">Download</a>
                                            }
                                        </td>*@
                                    <td>@item.CreatedDate</td>
                                    <td>
                                        <div class="table-actions text-center">
                                            @if (@item.AcceptedStatus == null || @item.AcceptedStatus == "")
                                            {
                                                <a asp-action="Delete" id="delete" asp-route-id=@item.EncryptedId data-bs-toggle="tooltip" title="" data-bs-placement="top" data-bs-original-title="Delete" onclick="return confirm('Are you sure you want to delete this?')"><i class="fe fe-trash fs-16 text-danger"></i></a>
                                            }
                                            else
                                            {
                                                <label style="font-weight:bold; color:green">Accepted</label>
                                            }
                                        </div>
                                    </td>

                                </tr>
                            }
                        </tbody>
                    </table>
                    @if (ViewBag.Message != null)
                    {
                        <script type="text/javascript">

            window.onload = function () {
                alert("@ViewBag.Message");
            };
                        </script>

                    }
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End Row -->

@section Scripts{

    <script src="~/assets/plugins/datatable/js/jquery.dataTables.min.js"></script>
    <script src="~/assets/plugins/datatable/js/dataTables.bootstrap5.js"></script>
    <script src="~/assets/plugins/datatable/js/dataTables.buttons.min.js"></script>
    <script src="~/assets/js/table-data.js"></script>
    <script src="~/assets/plugins/datatable/js/buttons.bootstrap5.min.js"></script>
    <script src="~/assets/plugins/datatable/js/jszip.min.js"></script>
    <script src="~/assets/plugins/datatable/pdfmake/pdfmake.min.js"></script>
    <script src="~/assets/plugins/datatable/pdfmake/vfs_fonts.js"></script>
    <script src="~/assets/plugins/datatable/js/buttons.html5.min.js"></script>
    <script src="~/assets/plugins/datatable/js/buttons.print.min.js"></script>
    <script src="~/assets/plugins/datatable/js/buttons.colVis.min.js"></script>
    <script src="~/assets/plugins/datatable/dataTables.responsive.min.js"></script>
    <script src="~/assets/plugins/datatable/responsive.bootstrap5.min.js"></script>
}
