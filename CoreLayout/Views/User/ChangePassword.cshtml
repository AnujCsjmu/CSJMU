@model CoreLayout.Models.UserManagement.RegistrationModel
@{
    ViewData["Title"] = "Create";
}
<!-- Page Header -->
<div class="page-header">
    <div>
        <h2 class="main-content-title tx-24 mg-b-5">Change Password</h2>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="#">Password</a></li>
            <li class="breadcrumb-item active" aria-current="page">Change</li>
        </ol>
    </div>

</div>
<!-- End Page Header -->
<!-- Conetnt Start-->
<div class="row">
    <div class="col-lg-12 col-md-12">
        <div class="card custom-card">
            <div class="card-body">
                @*<div>
                        <h6 class="card-title mb-1">Create State</h6>
                        <hr>
                    </div>*@

                <form asp-action="Create" method="post">
                    <div class="row">
                        <div asp-validation-summary="ModelOnly" class="text-danger">

                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="UserName"> </label><span class="tx-danger"> *</span>
                                <input asp-for="UserName" class="form-control" readonly="readonly" />
                                <span asp-validation-for="UserName" class="parsley-errors-list filled"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="LoginID"> </label><span class="tx-danger"> *</span>
                                <input asp-for="LoginID" class="form-control" readonly="readonly" />
                                <span asp-validation-for="LoginID" class="parsley-errors-list filled"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="MobileNo"> </label><span class="tx-danger"> *</span>
                                <input asp-for="MobileNo" class="form-control" maxlength="10" readonly="readonly" />
                                <span asp-validation-for="MobileNo" class="parsley-errors-list filled"></span>
                            </div>
                        </div>

                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="EmailID"> </label><span class="tx-danger"> *</span>
                                <input asp-for="EmailID" class="form-control" readonly="readonly" />
                                <span asp-validation-for="EmailID" class="parsley-errors-list filled"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="RoleId"> </label><span class="tx-danger"> *</span>
                                <input asp-for="RoleName" class="form-control" readonly="readonly" />
                                <span asp-validation-for="RoleId" class="parsley-errors-list filled"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="OldPassword"> </label><span class="tx-danger"> *</span>
                                <input asp-for="OldPassword" class="form-control" />
                                <span asp-validation-for="OldPassword" class="parsley-errors-list filled"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="Password"> </label><span class="tx-danger"> *</span>
                                <input asp-for="Password" class="form-control" />
                                <span asp-validation-for="Password" class="parsley-errors-list filled"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="ConfirmPassword"> </label><span class="tx-danger"> *</span>
                                <input asp-for="ConfirmPassword" class="form-control" />
                                <span asp-validation-for="ConfirmPassword" class="parsley-errors-list filled"></span>
                            </div>
                        </div>
                        <div class="form-group">
                            <input type="submit" asp-action="ChangePassword" class="btn btn-primary" value="Save" />
                            <a asp-action="Index" class="btn btn-success">Back to List</a>
                        </div>
                    </div>
                </form>

            </div>
        </div>
    </div>
</div>
<!-- Conetnt End-->
